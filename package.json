{
  "name": "flexkit",
  "version": "1.8.0",
  "description": "Dead-simple layout helpers for React applications",
  "main": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "test": "cross-env BABEL_ENV=commonjs mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch",
    "cover": "cross-env BABEL_ENV=commonjs istanbul cover _mocha -- --compilers js:babel-register --recursive",
    "lint": "eslint src test",
    "build": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build",
    "start": "webpack-dev-server"
  },
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/opvs/flexkit.git"
  },
  "keywords": [
    "flexbox",
    "flex",
    "layout",
    "styled-components",
    "components",
    "react",
    "ui",
    "npm",
    "module",
    "node",
    "javascript"
  ],
  "author": "Alex alex.freska@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/opvs/flexkit/issues"
  },
  "homepage": "https://github.com/opvs/flexkit#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "cross-env": "^3.0.0",
    "eslint": "^3.6.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "^6.3.0",
    "istanbul": "^1.0.0-alpha",
    "mocha": "^3.0.2",
    "rimraf": "^2.5.4",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "styled-components": "^2.2.3"
  }
}
